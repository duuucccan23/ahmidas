namespace Core
{
  template< typename Element, size_t L, size_t T >
  inline hcField< Element, L, T >::hcField(Field< Element, L, T > const &parent)
    : d_parent(parent)
  {}

  template< typename Element, size_t L, size_t T >
  inline Field< Element, L, T > const &hcField< Element, L, T >::dagger() const
  {
    return d_parent;
  }
}
