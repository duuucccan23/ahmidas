namespace Core
{
  template< typename Element, size_t L, size_t T >
  size_t Core::Field< Element, L, T >::moveBufferToData(size_t written)
  {
    std::copy(d_buffer, d_buffer + d_bufferSize / sizeof(Element), d_field + written);
    written += d_bufferSize / sizeof(Element);
    return written;
  }
}
