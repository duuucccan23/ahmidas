namespace Core
{
  template< typename Element >
  Field< Element >::const_iterator Field< Element >::const_iterator:::operator--(int)
  {
    iterator result(*this);
    --d_idx;
    return result;
  }

  template< typename Element >
  Field< Element >::const_iterator Field< Element >::const_iterator:::operator++(int)
  {
    iterator result(*this);
    ++d_idx;
    return result;
  }
}
